{
  "workspaces": [
    "Packages/*"
  ],
  "private": true,
  "devDependencies": {
    "@auto/bump": "^1.0.0",
    "@auto/git": "^1.0.0",
    "@auto/log": "^1.0.0",
    "@auto/npm": "^1.0.0",
    "@auto/start-plugin": "^1.0.0",
    "@auto/utils": "^1.0.0",
    "@bubble-dev/eslint-config": "^1.0.0",
    "@bubble-dev/start-preset": "^1.0.0",
    "@bubble-dev/typescript-config": "^1.0.0",
    "@start/plugin": "^1.0.0",
    "@start/plugin-sequence": "^1.0.0",
    "@types/node-fetch": "^2.5.5",
    "execa": "^4.0.0",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.5"
  },
  "start": {
    "file": "./tasks"
  },
  "auto": {
    "initialType": "minor",
    "autoNamePrefix": "@",
    "zeroBreakingChangeType": "major",
    "npm": {
      "registry": "http://localhost:4873",
      "access": "public"
    },
    "shouldWriteChangelogFiles": true
  },
  "eslintConfig": {
    "extends": "@bubble-dev/eslint-config"
  },
  "eslintIgnore": [
    "build/",
    "coverage/",
    "node_modules/"
  ]
}
